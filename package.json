    "express": "^4.17.1",
    "file-saver": "^2.0.2",
    "html2canvas": "^1.0.0-rc.5",
    "rxjs": "~6.4.0",
    "tslib": "^1.9.0",
    "typescript": "~3.4.3",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.801.0",

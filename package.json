    "@angular/core": "~8.1.0",
    "@angular/forms": "~8.1.0",
    "@angular/platform-browser": "~8.1.0",
    "@angular/platform-browser-dynamic": "~8.1.0",
    "@angular/router": "~8.1.0",
    "@ng-toolkit/serverless": "^8.0.3",
    "@types/file-saver": "^2.0.1",
    "@types/html2canvas": "0.0.35",
    "aws-serverless-express": "^3.2.0",
    "bootstrap": "^3.4.1",
    "cors": "~2.8.4",
